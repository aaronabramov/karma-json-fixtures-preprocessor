{
  "name": "karma-json-fixtures-preprocessor",
  "version": "0.0.6",
  "description": "A Karma plugin to compile JSON files and make them accessible from Javascript code",
  "main": "index.js",
  "scripts": {
    "test": "npm run mocha && npm run karma",
    "karma": "./node_modules/karma/bin/karma start ./karma_test/conf.js --single-run",
    "mocha": "./node_modules/mocha/bin/mocha --compilers js:babel/register mocha_test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dmitriiabramov/karma-json-fixtures-preprocessor.git"
  },
  "keywords": [
    "json",
    "karma",
    "preprocessor",
    "fixture",
    "fixtures",
    "test"
  ],
  "author": "Dmitrii Abramov",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dmitriiabramov/karma-json-fixtures-preprocessor/issues"
  },
  "homepage": "https://github.com/dmitriiabramov/karma-json-fixtures-preprocessor",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.2.3",
    "mocha": "^2.3.0",
    "phantomjs": "^1.9.19"
  }
}
